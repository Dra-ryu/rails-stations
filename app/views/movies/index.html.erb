<h1>ユーザー検索</h1>
<%= search_form_for @q, url: search_path do |f| %>
  <%= f.label :name_cont, 'タイトル名' %>
  <%= f.search_field :name_cont %>
  <br>
  <%= f.submit '検索' %>
<% end %>
<ul>
    <% @movies.each do |movie| %>
        <li>
            <img src=<%= movie.image_url %> >
            <h3><%= movie.name %></h3>
        </li>
    <% end %>
</ul>